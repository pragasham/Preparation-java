class Solution {
    public List<Integer> frequencyCount(int[] arr) {
        // code here
      ArrayList<Integer> hash=new ArrayList<>(Collections.nCopies(arr.length,0));
       for(int i=0;i<arr.length;i++){
           hash.set(arr[i]-1,hash.get(arr[i]-1)+1);
       }
        return hash;
    }
}
