// User function template for java

class Solution {
    int[][] rotateMatrix(int k, int mat[][]) {
        // code here
        int n=mat.length;
        int m=mat[0].length;
        if(k>m) k=k%m;
        int ans[][]= new int[n][m];
        for(int i=0;i<n;i++){
            for(int j=0;j<m;j++){
                int ne = (j-k+m) %m;
                ans[i][ne]=mat[i][j];
            }
        }
        return ans;
    }
}
