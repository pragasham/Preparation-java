class Solution {
    public int findFloor(int[] arr, int x) {
        // code here
        int index=-1;
        int a=0;
        int b=arr.length-1;
        while(a<=b){
            int mid=(a+b)/2;
            if(arr[mid]<=x){
                index=mid;
                a=mid+1;
            }
            else{
                b=mid-1;
            }
        }
        return index;
    }
}
