class Solution {
    public int reverseExponentiation(int n) {
        // code here
        int tem=n;
        int rev=0;
        while(tem!=0){
           int rem=tem%10;
           rev=rev*10+rem;
           tem/=10;
           
        }
        int result=(int)Math.pow(n,rev);
        return result;
    }
}
