class Solution {
    public static int intersectSize(int a[], int b[]) {
        // Your code here
        int count=0;
        Map<Integer,Integer>map=new HashMap<>();
        
        for(int num:a)map.put(num,1);
        for(int num:b){
            if(map.containsKey(num)){
                map.put(num,map.get(num)+1);
            }
        }
        for(int i:map.values()){
            if(i>1){
                count++;
            }
        }
        return count;
    }
}
